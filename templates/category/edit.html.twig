{% extends "base.html.twig" %}
{% block title %}
	Modification de
	{{category.name}}
{% endblock %}
{% block body %}
	<h1>Modifier
		{{category.name}}</h1>

	<div class="row">
		<div class="col">
			{{form_start(formView)}}
			{{form_row(formView.name)}}

			<button class="btn btn-primary">
				<i class="fas fa-save"></i>
				Enregistrer
			</button>

			{{form_end(formView)}}
		</div>
	</div>
{% endblock %}
