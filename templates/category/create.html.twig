{% extends 'base.html.twig' %}

{% block title %}
	Créer une catégorie
{% endblock %}
{% block body %}
	<h1>Créer une catégorie</h1>
	<div class="row">
		<div class="col">
			{{form_start(formView)}}
			{{form_row(formView.name)}}
			<button class="btn btn-primary" type="submit">
				<i class="fas fa-save"></i>
				Créer la catégorie</button>
			{{form_end(formView)}}
		</div>
	</div>
{% endblock %}
